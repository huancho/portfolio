---
import Heading from '../components/Heading.astro';
import ProjectCard from '../components/ProjectCard.astro';

const projects = [
  {
    name: 'Three.js Journey',
    description:
      'Passionate learner, currently exploring WebGL and shaders development through interactive courses by Bruno Simon.',
